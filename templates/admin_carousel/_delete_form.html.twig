<form method="post" action="{{ path('app_admin_carousel_delete', {'id': carousel.id}) }}" onsubmit="return confirm('Voulez vous vraiment supprimer cette image de carousel?');" class="d-inline">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ carousel.id) }}">
    <button class="btn btn-danger my-2"><i class="align-middle" data-feather="trash"></i></button>
</form>
