{% extends 'base-backend.html.twig' %}

{% block title %}Contenu{% endblock %}

{% block body %}
    <a href="{{ path('app_admin_contenu_index') }}" class="btn btn-dark btn-sm ">Retour Ã  la liste</a>
    <div class="d-flex justify-content-between align-items-center mb-3">
        <h1>Contenu</h1>
        {{ include('admin_contenu/_delete_form.html.twig') }}
    </div>
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contenu.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ contenu.titre }}</td>
            </tr>
            <tr>
                <th>Contenu</th>
                <td>{{ contenu.contenu }}</td>
            </tr>
            <tr>
                <th>Tag</th>
                <td>{{ contenu.tag }}</td>
            </tr>
            <tr>
                <th>RankOrder</th>
                <td>{{ contenu.rankOrder }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_contenu_edit', {'id': contenu.id}) }}" class="btn btn-success"><i class="align-middle" data-feather="edit-2"></i></a>

    {{ include('admin_contenu/_delete_form.html.twig') }}
{% endblock %}
