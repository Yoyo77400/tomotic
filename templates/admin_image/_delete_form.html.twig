<form method="post" action="{{ path('app_admin_image_delete', {'id': image.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');" class="d-inline">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ image.id) }}">
    <button class="btn btn-danger my-2"><i class="align-middle" data-feather="trash"></i></button>
</form>
