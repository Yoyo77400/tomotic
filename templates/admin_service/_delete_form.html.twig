<form method="post" action="{{ path('app_admin_service_delete', {'id': service.id}) }}" onsubmit="return confirm('Voulez vous vraimet supprimer ce service?');" class="d-inline">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ service.id) }}">
    <button class="btn btn-danger"><i class="align-middle" data-feather="trash"></i></button>
</form>
